    <label for="target">Choose a type:</label>
        <select id="target" name="category">

        <option value="" selected hidden disabled>Select...</option>
        <option value="One" >False Color Composition</option>
        <option value="Two">NDVI</option>
        <option value="Three">Locust Breeding Points</option>
        <option value="Four">Root Soil Moisture</option>
        <option value="Five">Wind Vector</option>
        <option value="Six">Accumulated Rainfall</option>
        <option value="Seven">Locust Infestation</option>
        <option value="Eight">Land Surface Temperature</option>
        <option value="Nine">Surface Soil Moisture</option>
        <option value="Ten">Leaf Index</option>
    </select>

    <div id="showOne" class="inv" >
        <div class="container">
            <div class="row">
                <div class = "col-md-8 col-center m-auto">
                    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                        <ol class="carousel-indicators">
                            <% items.forEach(function(slide, index){ %>
                                <% if(slide.category === "False Color Composition"){  %>
                                <li data-target="#carouselExampleIndicators" data-slide-to="<%= index %>" class=" <%= slide.class %>"></li>
                                <% } %>
                            <% }) %>
                        </ol>
                        <div class="carousel-inner">
                            <% items.forEach(function(slide){ %>
                                <% if(slide.category === "False Color Composition"){  %>
                                    <div class="carousel-item <%= slide.class %> ">
                                    
                                    <img class="d-block w-100" src="data:image/<%=slide.img.contentType%>;base64,
                                    <%=slide.img.data.toString('base64')%>">
                                        <p><%=slide.title%></p>       
                                  </div>
                               <% } %>
                                
                            <% }); %>  
                        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                          <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                          <span class="carousel-control-next-icon" aria-hidden="true"></span>
                          <span class="sr-only">Next</span>
                        </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
